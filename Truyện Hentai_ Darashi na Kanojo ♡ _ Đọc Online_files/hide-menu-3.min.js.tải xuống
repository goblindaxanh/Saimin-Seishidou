$(document).ready(function(){$(".a-drop-1").click(function(e){e.preventDefault(),$(".drop-1").toggle()}),$(document).mouseup(function(e){0==$(e.target).parent(".a-drop-1").length&&$(".drop-1").hide()}),$(".a-drop-2").click(function(e){e.preventDefault(),$(".drop-2").toggle()}),$(document).mouseup(function(e){0==$(e.target).parent(".a-drop-2").length&&$(".drop-2").hide()}),$(".a-drop-3").click(function(e){e.preventDefault(),$(".drop-3").toggle()}),$(document).mouseup(function(e){0==$(e.target).parent(".a-drop-3").length&&$(".drop-3").hide()}),$(".a-drop-4").click(function(e){e.preventDefault(),$(".drop-4").toggle()}),$(document).mouseup(function(e){0==$(e.target).parent(".a-drop-4").length&&$(".drop-4").hide()}),$(".a-drop-5").click(function(e){e.preventDefault(),$(".drop-5").toggle()}),$(document).mouseup(function(e){0==$(e.target).parent(".a-drop-5").length&&$(".drop-5").hide()}),$(".a-drop-10").click(function(e){e.preventDefault(),$(".drop-10").toggle()}),$(document).mouseup(function(e){0==$(e.target).parent(".a-drop-10").length&&$(".drop-10").hide()})});